{
    "name": "seandowney/backpackeventscrud",
    "description": "Events management interface for Laravel 5 using Backpack CRUD.",
    "keywords": [
        "laravel backpack",
        "events",
        "manage event"
    ],
    "homepage": "https://github.com/seandowney/laravel-backpack-events-crud",
    "license": "MIT",
    "authors": [
        {
            "name": "Se√°n Downey",
            "email": "sean@considerweb.com",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "backpack/crud": "3.4.*|3.5.*",
        "cviebrock/eloquent-sluggable": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "SeanDowney\\BackpackEventsCrud\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SeanDowney\\BackpackEventsCrud\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
